<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="@dimen/x_design_1500px"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_makesure_dialog"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_feed_title"
            style="@style/tv_feed_title_one"
            android:paddingTop="@dimen/y_design_30px"
            android:text="物料清单" />

        <include
            layout="@layout/tray_info_menu"
            android:layout_width="@dimen/x_design_1200px"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y_design_20px" />

        <android.support.v7.widget.RecyclerView
            android:id="@+id/recycyleview"
            android:layout_width="@dimen/x_design_1200px"
            android:layout_height="@dimen/y_design_412px">

        </android.support.v7.widget.RecyclerView>

        <TextView
            android:id="@+id/tv_cancle"
            style="@style/tv_next"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="@dimen/y_design_30px"
            android:layout_marginTop="@dimen/y_design_30px"
            android:text="关闭" />
    </LinearLayout>

</LinearLayout>